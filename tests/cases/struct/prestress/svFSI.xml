<?xml version="1.0" encoding="UTF-8" ?>
<svFSIFile version="0.1">

<GeneralSimulationParameters>
  <Continue_previous_simulation> 0 </Continue_previous_simulation>
  <Number_of_spatial_dimensions> 3 </Number_of_spatial_dimensions> 
  <Number_of_time_steps> 1000  </Number_of_time_steps> 
  <Time_step_size> 0.001 </Time_step_size> 
  <Spectral_radius_of_infinite_time_step> 0.0 </Spectral_radius_of_infinite_time_step> 
  <Searched_file_name_to_trigger_stop> STOP_SIM </Searched_file_name_to_trigger_stop> 

  <Save_results_to_VTK_format> 1 </Save_results_to_VTK_format> 
  <Name_prefix_of_saved_VTK_files> result </Name_prefix_of_saved_VTK_files> 
  <Increment_in_saving_VTK_files> 1 </Increment_in_saving_VTK_files> 
  <Start_saving_after_time_step> 1 </Start_saving_after_time_step> 

  <Increment_in_saving_restart_files> 1 </Increment_in_saving_restart_files> 
  <Convert_BIN_to_VTK_format> 0 </Convert_BIN_to_VTK_format> 

  <Verbose> 1 </Verbose> 
  <Warning> 0 </Warning> 
  <Debug> 0 </Debug> 

</GeneralSimulationParameters>


<Add_mesh name="msh" > 

  <Mesh_file_path> mesh/mesh-complete.mesh.vtu </Mesh_file_path>

  <Add_face name="wall_inner">
      <Face_file_path> mesh/mesh-surfaces/inner_surface.vtp </Face_file_path>
  </Add_face>

  <Add_face name="wall_outer">
      <Face_file_path> mesh/mesh-surfaces/outer_surface.vtp </Face_file_path>
  </Add_face>

  <Add_face name="wall_BCT">
      <Face_file_path> mesh/mesh-surfaces/branch_1.vtp </Face_file_path>
  </Add_face>

  <Add_face name="wall_LCC_LSA">
      <Face_file_path> mesh/mesh-surfaces/branch_2.vtp </Face_file_path>
  </Add_face>
  
  <Add_face name="wall_outlet">
      <Face_file_path> mesh/mesh-surfaces/outlet.vtp </Face_file_path>
  </Add_face>
  <Add_face name="wall_inlet">
      <Face_file_path> mesh/mesh-surfaces/inlet.vtp </Face_file_path>
  </Add_face>

</Add_mesh>


<Add_equation type="struct" > 

   <Coupled> true </Coupled>
   <Min_iterations> 1</Min_iterations>  
   <Max_iterations> 10 </Max_iterations> 
   <Tolerance> 1e-6 </Tolerance> 

   <Constitutive_model type="nHK"> </Constitutive_model>
   <Density> 0.00012 </Density> 
   <Elasticity_modulus> 600.0e3 </Elasticity_modulus>
   <Poisson_ratio> 0.49 </Poisson_ratio>

   <Output type="Spatial" >
     <Displacement> true </Displacement>
     <Stress> true </Stress>
   </Output>

   <LS type="BICG" >
      <Linear_algebra type="fsils" >
         <Preconditioner> fsils </Preconditioner>
      </Linear_algebra>
      <Tolerance> 1e-4 </Tolerance>
      <Max_iterations> 100 </Max_iterations> 
   </LS>

   <Add_BC name="wall_inner" > 
      <Type> Trac </Type> 
      <Time_dependence> Steady </Time_dependence> 
      <Traction_values_file_path> ./traction.vtp </Traction_values_file_path> 
   </Add_BC>

   <Add_BC name="wall_inlet" > 
      <Type> Dir </Type> 
      <Time_dependence> Steady </Time_dependence> 
      <Value> 0.0 </Value> 
      <Impose_on_state_variable_integral> true </Impose_on_state_variable_integral> 
   </Add_BC> 
   
   <Add_BC name="wall_outlet" > 
      <Type> Dir </Type> 
      <Time_dependence> Steady </Time_dependence> 
      <Value> 0.0 </Value> 
      <Impose_on_state_variable_integral> true </Impose_on_state_variable_integral> 
   </Add_BC> 
   
   <Add_BC name="wall_BCT" > 
      <Type> Dir </Type> 
      <Time_dependence> Steady </Time_dependence> 
      <Value> 0.0 </Value> 
      <Impose_on_state_variable_integral> true </Impose_on_state_variable_integral> 
   </Add_BC> 
   
   <Add_BC name="wall_LCC_LSA" > 
      <Type> Dir </Type> 
      <Time_dependence> Steady </Time_dependence> 
      <Value> 0.0 </Value> 
      <Impose_on_state_variable_integral> true </Impose_on_state_variable_integral> 
   </Add_BC> 



</Add_equation>   

</svFSIFile>


